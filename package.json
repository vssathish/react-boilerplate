{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "React boilerplate inspired from react-slingshot and ps-redux",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "preinstall": "node tools/nodeVersionCheck.js",
    "remove-demo": "babel-node tools/removeDemo.js",
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message remove-dist",
    "start": "npm-run-all --parallel open:srv",
    "open:src": "nodemon --exec babel-node tools/srcServer.js",
    "open:srv": "babel-node tools/server.js",
    "open:dist": "env NODE_ENV=production babel-node tools/server.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "rimraf ./dist",
    "prebuild-old": "npm run clean-dist && npm run lint && npm run test",
    "prebuild": "npm run clean-dist",
    "build": "babel-node tools/build.js",
    "build-local": "babel-node tools/build.js && npm run open:dist",
    "test": "mocha tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover": "babel-node node_modules/isparta/bin/isparta cover --root src --report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" --reporter progress",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require ./tools/testSetup.js \"src/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "test:watch": "npm run test -- --watch",
    "open:cover": "npm run test:cover && open coverage/index.html",
    "build-server": "babel -d ./server ./tools"
  },
  "author": "Sathish Sridharan",
  "license": "MIT",
  "dependencies": {
    "axios": "0.13.1",
    "babel-polyfill": "6.13.0",
    "bunyan": "1.8.1",
    "classnames": "2.2.5",
    "compression": "1.6.2",
    "core-decorators": "0.12.3",
    "curlrequest": "0.5.7",
    "jssha": "2.2.0",
    "lodash": "^4.0.0",
    "merge": "1.2.0",
    "mobx": "2.4.1",
    "mobx-react": "3.5.3",
    "mobx-react-devtools": "4.2.4",
    "moment": "2.14.1",
    "object-assign": "4.1.0",
    "react": "0.14.7",
    "react-bootstrap": "0.29.5",
    "react-bootstrap-validation": "0.1.11",
    "react-copy-to-clipboard": "4.2.2",
    "react-dom": "0.14.7",
    "react-loader": "2.0.0",
    "react-overlays": "0.6.6",
    "react-redux": "4.4.0",
    "react-router": "2.0.0",
    "react-router-bootstrap": "0.20.1",
    "react-waypoint": "3.1.1",
    "redux": "3.3.1",
    "redux-logger": "2.6.1",
    "redux-thunk": "1.0.3",
    "revalidator": "0.3.1"
  },
  "devDependencies": {
    "autoprefixer": "6.3.7",
    "babel": "5.8.23",
    "babel-eslint": "4.1.7",
    "babel-loader": "5.1.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-react-transform": "1.1.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "browser-sync": "2.13.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.2.0",
    "copy-webpack-plugin": "3.0.1",
    "coveralls": "2.11.11",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "enzyme": "2.4.1",
    "eslint": "3.1.1",
    "eslint-plugin-import": "1.11.1",
    "eslint-plugin-jsx-a11y": "2.0.1",
    "eslint-plugin-react": "5.2.2",
    "eslint-watch": "2.1.13",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "isparta": "4.0.0",
    "mocha": "2.5.3",
    "node-sass": "3.8.0",
    "npm-run-all": "2.3.0",
    "open": "0.0.5",
    "postcss-loader": "0.9.1",
    "prompt": "1.0.0",
    "react-transform-hmr": "1.0.4",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.0",
    "sinon": "1.17.4",
    "sinon-chai": "2.8.0",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.1",
    "webpack-md5-hash": "0.0.5"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": ""
  }
}
